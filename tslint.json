{
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [true, 4],
        "semicolon": {
            "options": "never"
        },
        "quotemark": {
            "options": ["single", "jsx-double"]
        },
        "arrow-parens": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "no-var-requires": false,
        "no-console": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}